						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const responseJson = pm.response.json();",
									"    if (responseJson.access_token) {",
									"        pm.environment.set('token', responseJson.access_token);",
									"        console.log('Token saved to environment');",
									"    }",
									"}"
								],
